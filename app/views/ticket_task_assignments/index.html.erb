<p id="notice"><%= notice %></p>

<h1>Listing Ticket Task Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Ticket task</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_task_assignments.each do |ticket_task_assignment| %>
      <tr>
        <td><%= ticket_task_assignment.ticket_task_id %></td>
        <td><%= ticket_task_assignment.user_id %></td>
        <td><%= link_to 'Show', ticket_task_assignment %></td>
        <td><%= link_to 'Edit', edit_ticket_task_assignment_path(ticket_task_assignment) %></td>
        <td><%= link_to 'Destroy', ticket_task_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket task assignment', new_ticket_task_assignment_path %>
